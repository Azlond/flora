                        "type": "mysql",
                        "database": "contents",
                        "table": "article_comment"
                    },
                    "likes": {
                        "type": "mysql",
                        "database": "contents",
                        "table": "comment_likes"
                    }
                },
                "attributes": {
                    "id": {
                        "type": "int",
                        "map": {
                            "default": {
                                "primary": "id",
                                "likes": "commentId"
                            }
                        },
                        "filter": [
                            "equal"
                        ]
                    },
                    "user": {
                        "resource": "user",
                        "parentKey": {
                            "primary": [
                                "userId"
                            ]
                        },
                        "childKey": null
                    },
                    "content": {
                        "map": {
                            "default": {
                                "primary": "content"
                            }
                        }
                    },
                    "likes": {
                        "type": "int",
                        "map": {
