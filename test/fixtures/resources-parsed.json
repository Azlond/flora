                    "MOCK": "Select AST of 'author[firstname,lastname],categories.name,secretInfo'"
                },
                "value": ""
            },
            "secretInfo": {
                "internal": true,
                "map": {
                    "default": {
                        "primary": "secretInfo"
                    }
                }
            },
