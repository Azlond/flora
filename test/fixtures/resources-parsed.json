                            }
                        }
                    }
                },
                "resolvedPrimaryKey": {
                    "primary": [
                        "id"
                    ]
                }
            },
            "body": {
                "map": {
                    "default": {
                        "articleBody": "body"
                    }
                }
            },
            "video": {
                "parentKey": null,
                "childKey": {
                    "primary": [
                        "articleId"
                    ]
                },
                "dataSources": {
                    "primary": {
                        "type": "mysql",
                        "database": "contents",
                        "table": "article_video"
                    }
                },
                "attributes": {
                    "url": {
                        "map": {
                            "default": {
                                "primary": "url"
                            }
                        }
                    },
                    "previewImage": {
                        "map": {
